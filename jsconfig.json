{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["app/components/*"],
      "@constants/*": ["app/constants/*"],
      "@hooks/*": ["app/hooks/*"],
      "@screens/*": ["app/screens/*"],
      "@schemas/*": ["app/schemas/*"],
      "@config/*": ["app/config/*"],
      "@utils/*": ["app/utils/*"],
      "@api/*": ["app/api/*"],
      "@assets/*": ["assets/*"]
    }
  },
  "exclude": ["node_modules"]
}
